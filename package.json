{
  "name": "acertc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server",
    "start:prod": "./scripts/run-serve.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "TZ=Asia/Vladivostok yarn test:coverage && cat ./coverage/lcov.info | coveralls",
    "build": "webpack",
    "build:prod": "NODE_ENV=production webpack -p",
    "build:watch": "webpack --watch",
    "deploy": "./scripts/deploy.sh",
    "lint": "eslint --ext .js --ext .jsx src/"
  },
  "lint-staged": {
    "src/*/**/*.{js,jsx}": [
      "eslint --ext .js --ext .jsx --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/raf.shim.js",
      "<rootDir>/config/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/*/**/*.{js,jsx}",
      "!src/utilities/*",
      "!src/components/**/index.js",
      "!src/reducers/index.js",
      "!src/middleware/logger.js",
      "!src/index.jsx",
      "!src/store.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "brace": "^0.11.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "jest-css-modules": "^1.1.0",
    "lodash": "^4.17.4",
    "lodash.map": "^4.6.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-ace": "^5.5.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.3",
    "react-icons": "^2.2.7",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.6",
    "react-redux-fetch": "0.11.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "^0.0.3",
    "react-router-transition": "^1.1.2",
    "react-split-pane": "^0.1.74",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-multi": "^0.1.12",
    "style-loader": "^0.19.0",
    "styled-components": "^2.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-sprite-loader": "^3.4.1"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "optimize-js-plugin": "0.0.4",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "serve": "^6.4.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.0"
  }
